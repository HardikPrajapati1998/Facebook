<html>
    <body>
        <table id="example" class="table table-striped table-bordered" border="1px" >
            <% if(user_array.length>0) { %>
        <thead>
            <tr>
                <th class="wd-15p">No</th>
              
                <th class="wd-15p">Email Id</th>
            <th >Password</th>
            
            </tr>
        </thead>
         
        <tbody>
                <% for(var i=0 ;i<user_array.length;i++) {%>
            <tr>
                    <td><%= i+1 %></td>
                 
                    <td><%=  user_array[i].User_email %></td>
                    <td><%=  user_array[i].User_password %></td>
                    
                    
            </tr>
            <% } %>
        </tbody>
        <% } else {%>
            Not Record Found
            <% } %>
    </table>
    </body>
</html>